{
  "name": "electron-prisma-trpc-example",
  "version": "1.3.0",
  "repository": "https://github.com/awohletz/electron-prisma-trpc-example-releases",
  "description": "An example repo showing how to use Electron with tRPC and Prisma",
  "main": "dist/server/main.js",
  "workspaces": {
    "nohoist": [
      "*"
    ]
  },
  "scripts": {
    "build": "vite build && prisma generate && node copy-files.js && tsc --build && node install-engines-on-mac.js",
    "start": "pnpm run build && cross-env NODE_ENV=development electron .",
    "pack": "pnpm run build && electron-builder --dir",
    "dist": "pnpm run build && electron-builder",
    "publish": "pnpm run build && dotenv -- electron-builder -p always"
  },
  "author": "Ayron Wohletz",
  "license": "ISC",
  "devDependencies": {
    "@electron/notarize": "^1.2.1",
    "@prisma/engines": "^4.10.1",
    "@types/node": "^18.11.17",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "electron": "^23.1.0",
    "electron-builder": "^23.6.0",
    "fs-extra": "^11.1.0",
    "replace-in-file": "^6.3.5",
    "typescript": "^4.9.4",
    "vite": "^4.1.3"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
    "@trpc/client": "^10.5.0",
    "@trpc/server": "^10.5.0",
    "electron-log": "^4.4.8",
    "prisma": "^4.10.1",
    "superjson": "^1.12.1",
    "zod": "^3.20.2"
  }
}
