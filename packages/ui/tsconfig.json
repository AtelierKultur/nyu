{
  "extends": "tsconfig/frontend.json",
  "include": [
    "src",
    "./.eslintrc.cjs"
  ],
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@containers/*": [
        "./src/containers/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@ducks/*": [
        "./src/ducks/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@interfaces/*": [
        "./src/components/interfaces/*"
      ],
      "@images/*": [
        "./src/images/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@selectors/*": [
        "./src/selectors/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  }
}