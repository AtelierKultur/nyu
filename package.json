{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "db:migrate:deploy": "turbo run db:migrate:deploy",
    "db:push": "turbo run db:push",
    "db:seed": "turbo run db:seed",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate": "turbo run generate",
    "lint": "turbo run lint"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redux-form": {
      "react-redux": "8.0.5"
    }
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "tsx": "^3.7.1",
    "turbo": "^1.7.4"
  }
}
